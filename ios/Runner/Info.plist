<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Memories</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>Memories</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>NSFaceIDUsageDescription</key>
	<string>Use Face ID to quickly and securely sign in to your account</string>
	<key>NSSpeechRecognitionUsageDescription</key>
	<string>Memories uses speech recognition to convert your voice recordings into text for your memories and stories.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Memories needs access to your microphone to record voice stories and memories.</string>
	<key>NSCameraUsageDescription</key>
	<string>Memories needs access to your camera to capture photos and videos for your memories.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Memories needs access to your photo library to select photos and videos for your memories.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>Memories needs access to save photos and videos to your photo library.</string>
	<key>NSLocalNetworkUsageDescription</key>
	<string>Memories uses local network access to check connectivity status and sync your memories.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Memories needs access to your location to tag your memories with where they happened.</string>
	<key>NSBonjourServices</key>
	<array>
		<string>_dartobservatory._tcp</string>
	</array>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>com.memories.app.beta</string>
			</array>
		</dict>
	</array>
</dict>
</plist>
